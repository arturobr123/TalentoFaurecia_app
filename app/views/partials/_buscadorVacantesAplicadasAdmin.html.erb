

<div class="row left-xs">

	<div class="col-xs-8">

		<div class="box">


			<div class="">
		     <%=form_tag "/search/search_vacantes_aplicadas_Admin", method: :get, remote: true, id:"search-form" do  %>

		      <div class="input-field col s2">
		      	<%=text_field_tag :keywordUserName, nil, placeholder: "Por nombre ", class:"labelBuscador" %>
		      </div>

		      <div class="input-field col s2">
		      	<%=text_field_tag :keywordUser_1_lastname, nil, placeholder: "apellido paterno", class:"labelBuscador" %>
		      </div>

		      <div class="input-field col s2">
		      	<%=text_field_tag :keywordUser_2_lastname, nil, placeholder: "apellido materno", class:"labelBuscador" %>
		      </div>


		      <div class="input-field col s4">
		      	<%=text_field_tag :keywordVacanteName, nil, placeholder: "Por nombre de la vacante", class:"labelBuscador" %>

		      </div>
		 

		      <div class="input-field col s2">
		      	<button class="waves-effect waves-light btn buttonBuscador" type="submit">Buscar</button>

		      </div>


          	   	  

        	<% end %>
			</div>



		</div>

	</div>

</div>

